; ModuleID = 'MiniC'
source_filename = "MiniC"

@arr = private global [20 x i8] c"$$$$$$$$$$$$$$$$$$$$"
@0 = private unnamed_addr constant [3 x i8] c"%c\00", align 1

declare i32 @printf(i8*, ...)

declare i32 @scanf(i8*, ...)

define i32 @main() {
entry:
  %0 = load [20 x i8], [20 x i8]* @arr, align 1
  %printf = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([3 x i8], [3 x i8]* @0, i32 0, i32 0), [20 x i8] %0)
  ret i32 0
}
